{
  "name": "personal-website",
  "description": "Piotr Bęc - personal website",
  "version": "1.0.0",
  "author": "Piotr Bęc <helloworld@becpiotr.dev",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=en",
    "start-pl": "ng serve --configuration=pl",
    "start-de": "ng serve --configuration=de",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run personal-website:serve-ssr",
    "serve:ssr": "node dist/personal-website/proxy-server.js",
    "build:ssr": "ng build && ng run personal-website:server && cp proxy-server.js dist/personal-website",
    "prerender": "ng run personal-website:prerender",
    "i18n": "ng extract-i18n && xliffmerge --profile xliffmerge.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/cdk": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/material": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/platform-server": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@angular/service-worker": "^14.0.0",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.0",
    "@fortawesome/free-brands-svg-icons": "^6.1.0",
    "@fortawesome/free-regular-svg-icons": "^6.1.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.0",
    "@nguniversal/express-engine": "^13.0.2",
    "chart.js": "^3.8.0",
    "express": "^4.15.2",
    "hammerjs": "^2.0.8",
    "ng2-charts": "^3.0.11",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.0",
    "@angular/cli": "^14.0.0",
    "@angular/compiler-cli": "^14.0.0",
    "@angular/localize": "^14.0.0",
    "@nguniversal/builders": "^13.0.2",
    "@types/express": "^4.17.0",
    "@types/hammerjs": "^2.0.41",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "autoprefixer": "^10.4.4",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ngx-i18nsupport": "^0.17.1",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.23",
    "typescript": "~4.7.3"
  }
}